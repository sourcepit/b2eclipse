<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="B2Builder"
         name="B2 Project Builder"
         point="org.eclipse.core.resources.builders">
      <builder
            hasNature="true">
         <run
               class="org.sourcepit.b2eclipse.core.builder.B2Builder">
         </run>
      </builder>
   </extension>
   <extension
         id="B2Nature"
         name="B2 Project Nature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="org.sourcepit.b2eclipse.core.B2Nature">
         </run>
      </runtime>
      <builder
            id="org.sourcepit.b2eclipse.core.B2Builder">
      </builder>
   </extension>
   <extension
         id="xmlProblem"
         name="XML Problem"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.eclipse.core.resources.problemmarker">
      </super>
      <persistent
            value="true">
      </persistent>
   </extension>

</plugin>
